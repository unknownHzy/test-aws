{
    "family": "fortress-hangzhou-test-definition",
    "containerDefinitions": [
        {
            "name": "fortress-hangzhou-test",
            "image": "298117917676.dkr.ecr.us-east-2.amazonaws.com/fortress-hangzhou-dev:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "fortress-hangzhou-api-test-9999-tcp",
                    "containerPort": 9999,
                    "hostPort": 9999,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "pseudoTerminal":true,
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fortress-hangzhou-test",  // 需要被创建在cloud-watch
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::298117917676:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::298117917676:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "tags": [
        {
            "key": "Name",
            "value": "Fortress-Hangzhou-Test"
        }
    ]
}